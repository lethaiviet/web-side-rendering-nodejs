{
  "name": "web-side-rendering-nodejs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "node ./dist/bin/www.js",
    "build": "npm-run-all clean transpile",
    "transpile": "babel ./src -d dist",
    "clean": "rimraf dist",
    "prod": "npm-run-all build server",
    "dev": "nodemon --exec babel-node ./src/bin/www.js"
  },
  "lint-staged": {
      "src/**": "prettier --write"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-handlebars": "^6.0.6",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "nodemon": "^2.0.16"
  }
}
